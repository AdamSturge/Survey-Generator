#UserControlPanel{
	position: absolute;
	top: 820px;
	left: 0px;
	width: 950px;
	height: 80px;
	transform: skew(-25deg);
	-webkit-transform: skew(-25deg);
	-moz-transform: skew(-25deg);
	-ms-transform: skew(-25deg);
	-o-transform: skew(-25deg);
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	box-shadow:0 0 0 1px #eee inset, 0 0 15px rgba(250, 250, 100, 0.6);
	background:rgba(180,180,180,0.4);
	border: 1px solid rgba(51,51,51,0.8);
	-webkit-box-shadow:  2px 4px 2px rgba(250, 250, 100, 0.6);
    box-shadow:  2px 4px 4px 2px rgba(250, 250, 100, 0.6);
}

#UserControlPanel *{
	-webkit-transform: skew(25deg);
	-moz-transform: skew(25deg);
	-ms-transform: skew(25deg);
	-o-transform: skew(25deg);
}

.UserControlPanelButton{
	position:absolute;
	top: -50px;
	cursor: pointer;
	width: 100px;
	height: 100px;

	-moz-border-radius:7px;
	-webkit-border-radius:7px;
	border-radius:7px;
}

#manageSurveyButton{
	left: 100px;
}

#manageUserSurveyButton{
	left: 250px;
}

#createNewSurveyButton{
	left: 400px;
}

#logoutButton{
	left: 690px;
}


#systemMsg{
	color: orange;
	font-weight:bold;
	font-size: 15px;
	text-align: right;
}

#titlePanel{
	height: 100px;
	padding-bottom: 20xp;
}

#titleText{
	color: rgb(252, 236, 172);
}


#UserContentPanel{
	height: 580px;
	-moz-border-radius:7px;
	-webkit-border-radius:7px;
	border-radius:7px;
	-moz-box-shadow:0 0 0 1px #eee inset, 0 0 15px rgba(0,0,0,0.3);
	-webkit-box-shadow:0 0 0 1px #eee inset, 0 0 15px rgba(0,0,0,0.3);
	box-shadow:0 0 0 1px #eee inset, 0 0 15px rgba(0,0,0,0.3);
	background:rgba(180,180,180,0.4);
	border:1px solid rgba(51,51,51,0.8);
	padding: 20px;
}

#UserContentPanel p{
	font-size: 25px;
}
#UserContentPanel h3{
	text-align:center;
}
#UserContentPanel label{
	font-size: 30px;
	font-weight: bolder;
}

#manageSurveyPanel, #manageUserSurveyPanel, #createNewSurveyPanel{
	display: none;
}

/************** Manage Survey Panel  ***************/

#surveyListTable, #matchResultTable{
	font-size: 20px;
	margin-left: auto;
	margin-right: auto;
}
#surveyListTable th, #matchResultTable .titleItem div{
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	background-color:orange;
}

#matchResultTable .titleItem div, #matchResultTable .matchItem div{
	display: inline-block;
	margin: 1px;
}

#surveyListTable td, #matchResultTable .matchItem div{
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	background-color: rgb(235, 233, 205);
}
#surveyListTable td:hover, #matchResultTable .matchItem div:hover{
	background-color: rgb(146, 214, 206);
}

.adminSurveyListTitle, .userSurveyListTitle, .userJobfairTitle, #matchResultTable .titleItem .match_uid, #matchResultTable .matchItem .match_uid{
	text-align: Left;
	width: 300px;
	padding-left: 10px;
}

.userJobfairTitle{
	width: 150px;
}

td.adminSurveyListTitle, td.userSurveyListTitle{
	cursor: pointer;
}

#surveyListTable .surveyListAction, #matchResultTable .titleItem .match_score, #matchResultTable .matchItem .match_score{
	text-align:center;
	width: 100px;
}

/*********** Manage User Survey Panel **********/
#userSurveyListTable{
	font-size: 20px;
	margin-left: auto;
	margin-right: auto;
}
#userSurveyListTable th{
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	background-color:orange;
}

#userSurveyListTable td{
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	background-color: rgb(235, 233, 205);
}
#userSurveyListTable td:hover{
	background-color: rgb(146, 214, 206);
}

#userSurveyListTable .surveyListTitle{
	text-align: Left;
	width: 400px;
	padding-left: 10px;
}

#userSurveyListTable .surveyListAction{
	text-align:center;
	width: 100px;
}

